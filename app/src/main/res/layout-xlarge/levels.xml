<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/levels_layout_dawn"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/levels_bg_senen"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/bg_senen" />

    <LinearLayout
        android:id="@+id/container1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <LinearLayout
                android:id="@+id/container3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/back_btn_for_levels"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="70dp"
                    android:layout_marginBottom="15dp"
                    android:text="@string/back_btn_for_levels"
                    android:background="@drawable/startbtn_white95_press_gray100"
                    android:textSize="40sp"
                    android:textColor="@color/white95"
                    android:textAllCaps="false"/>
            </LinearLayout>

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp">

                <LinearLayout
                    android:id="@+id/container5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/container4_line1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/level1"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:gravity="center"
                            android:text="1"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/level2"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:gravity="center"
                            android:text="2"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/level3"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:gravity="center"
                            android:text="3"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/level4"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:gravity="center"
                            android:text="4"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/level5"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:gravity="center"
                            android:text="5"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/container4_line2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/level6"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:text="6"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold"
                            android:gravity="center"/>

                        <TextView
                            android:id="@+id/level7"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:text="7"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold"
                            android:gravity="center"/>

                        <TextView
                            android:id="@+id/level8"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:text="8"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold"
                            android:gravity="center"/>

                        <TextView
                            android:id="@+id/level9"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:text="9"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold"
                            android:gravity="center"/>

                        <TextView
                            android:id="@+id/level10"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:text="10"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/levelsbtn_white95_oress_gray100"
                            android:textColor="@color/black95"
                            android:textSize="45dp"
                            android:textStyle="bold"
                            android:gravity="center"/>
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>